<Application x:Class="TerrariViewer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:cv="clr-namespace:TerrariViewer.Converters"
             StartupUri="UI/MainWindow.xaml">
    <Application.Resources>
        
        <!-- Converters -->
        <cv:HairConverter x:Key="HairConverter" />
        <cv:FillColorConverter x:Key="FillColorConverter" />
        <cv:PrefixConverter x:Key="PrefixConverter" />
        <cv:StackConverter x:Key="StackConverter" />
        <cv:ItemConverter x:Key="ItemConverter" />
        <cv:BuffConverter x:Key="BuffConverter" />

    </Application.Resources>
</Application>
